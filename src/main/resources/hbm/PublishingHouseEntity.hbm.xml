<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="models.Publishinghouse" table="publishinghouse" schema="public" catalog="StoreBooks">
        <id name="id_publishing_house" column="id_publishing_house"/>
        <property name="name_publishing_house" column="name_publishing_house"/>
    </class>
</hibernate-mapping>