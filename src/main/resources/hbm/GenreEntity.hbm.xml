<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="models.Genre" table="genre" schema="public" catalog="StoreBooks">
        <id name="id_genre" column="id_genre"/>
        <property name="name_genre" column="name_genre"/>
    </class>
</hibernate-mapping>